{
  "compilerOptions": {
    "target": "es6",
    "esModuleInterop": true,
    "module": "es6",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "noImplicitAny": false,
    "downlevelIteration": true,
    "jsx": "react",
    "suppressImplicitAnyIndexErrors": true,
    "baseUrl": ".",
    "paths": {
      "app/*": [
        "web/app/*",
      ],
      "helpers/*": [
        "web/helpers/*",
      ],
      "interfaces/*": [
        "web/interfaces/*",
      ],
      "implementations/*": [
        "implementations/*",
      ],
    }
  },
  "exclude": [
    "node_modules"
  ],
  "types": [
    "requirejs"
  ]
}
