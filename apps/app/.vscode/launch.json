{
    "version": "0.1.0",
    "configurations": [
        {
            "name": "Attach Chrome",
            "type": "chrome",
            "request": "attach",
            "port": 9222,
            "url": "http://127.0.0.1:8111/#studio/print",
            "webRoot": "${workspaceRoot}/public"
        },
        {
            "name": "Attach",
            "type": "nwjs",
            "request": "attach",
            "port": 9222,
            "nwjsVersion": "any",
            "webRoot": "${workspaceRoot}"
        },
        {
            "name": "Launch",
            "type": "nwjs",
            "request": "launch",
            "webRoot": "${workspaceRoot}/public",
            "nwjsVersion": "0.18.7"
        }
    ]
}
