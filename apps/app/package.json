  "packageManager": "pnpm@10.0.0",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "@sentry/cli",
      "@swc/core",
      "chromedriver",
      "core-js",
      "core-js-pure",
      "cypress",
      "edgedriver",
      "electron",
      "electron-chromedriver",
      "font-scanner",
      "geckodriver",
      "nx",
      "spectron"
    ]
  },
  "build": {
    "appId": "com.flux3dp.beam-studio-flux",
    "extraMetadata": {
      "name": "beam-studio"
    },
    "protocols": {
      "name": "beam-studio-protocol",
      "schemes": [
        "beam-studio"
      ]
    },
    "generateUpdatesFilesForAllChannels": true,
    "extraResources": [
      "backend",
      "utils"
    ],
    "files": [
      "!**/*.{jsx,scss}"
    ],
    "publish": [
      {
        "provider": "s3",
        "bucket": "beamstudio",
        "path": "/${os}${env.PUBLISH_PATH}"
      }
    ],
    "mac": {
      "category": "com.flux3dp.beam-studio-flux",
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "build/entitlements.mac.plist",
      "entitlementsInherit": "build/entitlements.mac.plist",
      "extendInfo": {
        "NSCameraUsageDescription": "Beam Studio need to use USB camera to preview certain devices.",
        "NSLocalNetworkUsageDescription": "Beam Studio need to use local network to connect to certain devices."
      },
      "target": [
        "dmg",
        "zip"
      ],
      "icon": "public/logo.icns",
      "notarize": {
        "teamId": "4Y92JWKV94"
      },
      "asarUnpack": "**/*.node"
    },
    "linux": {
      "category": "Graphics",
      "description": "Beam Studio",
      "vendor": "FLUX Inc.",
      "target": [
        "deb",
        "AppImage"
      ],
      "icon": "app-icons"
    },
    "win": {
      "icon": "app-icons/win-icon.ico",
      "signtoolOptions": {
        "sign": "./customCodeSign.windows.js",
        "certificateSubjectName": "FLUX TECHNOLOGY INC.",
        "publisherName": "FLUX TECHNOLOGY INC."
      }
    },
    "dmg": {
      "artifactName": "Beam Studio ${version}.${ext}",
      "sign": false
    },
    "deb": {
      "depends": [
        "libfontconfig1-dev"
      ]
    },
    "nsis": {
      "include": "win-install.nsh",
      "artifactName": "Beam Studio Installer ${version}.${ext}",
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true
    },
    "fileAssociations": [
      {
        "ext": "beam",
        "description": "Edit and cut image files",
        "role": "Editor"
      },
      {
        "ext": "bvg",
        "description": "Edit and cut image files",
        "role": "Editor"
      },
      {
        "ext": "svg",
        "description": "Edit and cut image files",
        "role": "Editor"
      },
      {
        "ext": "jpg",
        "description": "Edit and cut image files",
        "role": "Editor"
      },
      {
        "ext": "png",
        "description": "Edit and cut image files",
        "role": "Editor"
      }
    ]
  },
  "productName": "Beam Studio"
}
