{
  "name": "beam-studio",
  "productName": "Beam Studio",
  "version": "1.9.5-alpha",
  "author": {
    "name": "FLUX Inc.",
    "email": "support@flux3dp.com"
  },
  "description": "Beam Studio",
  "private": true,
  "main": "electron-main.js",
  "dependencies": {
    "@ant-design/cssinjs": "^1.3.2",
    "@ant-design/icons": "^5.0.1",
    "@electron/notarize": "^1.2.3",
