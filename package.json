{
  "name": "beam-studio",
  "productName": "Beam Studio",
  "version": "1.3.2-alpha",
  "author": {
    "name": "FLUX Inc.",
    "email": "support@flux3dp.com"
  },
  "description": "Beam Studio",
  "private": true,
  "main": "electron-main.js",
  "dependencies": {
    "@simonwep/pickr": "^1.2.6",
    "bufferpack": "^0.0.6",
    "classnames": "^2.2.6",
    "cropperjs": "^1.4.1",
    "custom-electron-titlebar": "^3.2.2",
    "electron-notarize": "^0.2.0",
    "electron-updater": "^4.2.0",
    "font-manager": "^0.3.0",
    "fontkit": "^1.7.7",
    "jimp": "^0.10.3",
    "jquery": "^1.11.0",
    "jsdom-global": "^1.6.2",
    "nan": "^2.4.0",
    "net-ping": "^1.2.3",
    "node-machine-id": "^1.1.12",
    "react": "^16.9.0",
    "react-contextmenu": "^2.13.0",
    "react-dom": "^16.13.0",
    "serialport": "^8.0.5",
    "socket.io-client": "^1.4.6",
    "squirejs": "^0.2.1",
    "sudo-prompt": "^9.0.0",
    "text-to-svg": "jeffontheground/text-to-svg#e4fb3dc1ac9c8e5a2440373e7e018f07653ad574",
    "ws": "^7.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "ajv": "^5.5.2",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^6.2.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-react-jsx": "^6.4.0",
    "babel-preset-es2015": "~6.9.0",
    "babel-preset-react": "~6.5.0",
    "babel-preset-stage-0": "^6.3.13",
    "babel-preset-stage-2": "^6.3.13",
    "chai": "3.5.0",
    "electron": "5.0.6",
    "electron-builder": "22.3.6",
    "electron-publisher-s3": "^19.53.1",
    "electron-rebuild": "^1.8.5",
    "electron-reload": "~1.1.0",
    "eslint": "^4.9.0",
    "eslint-plugin-react": "^7.4.0",
    "expect": "^1.14.0",
    "gulp": "^4.0.2",
    "gulp-babel": "8.0.0",
    "gulp-clean-css": "~2.0.10",
    "gulp-exec": "~2.1.2",
    "gulp-mocha": "~2.2.0",
    "gulp-react": "~3.1.0",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "~1.6.0",
    "gulp-uglify": "~1.5.4",
    "gulp-webserver": "~0.9.1",
    "jsdom": "~7.2.2",
    "mocha": "^2.3.4",
    "mocha-jsdom": "~1.0.0",
    "pump": "~1.0.1",
    "q": "~1.4.1",
    "react-addons-test-utils": "^0.14.7",
    "requirejs": "~2.1.22",
    "style-loader": "^0.13.0",
    "webpack": "1.13.1"
  },
  "scripts": {
    "test": "mocha '_test/unit/slicer.js' --compilers js:babel-core/register",
    "start": "electron .",
    "start-debug": "electron . --debug",
    "start-server": "electron . --server",
    "postinstall": "electron-rebuild",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.flux3dp.beam-studio-flux",
    "generateUpdatesFilesForAllChannels": true,
    "afterSign": "./scripts/notarize.js",
    "extraResources": [
      "backend",
      "utils"
    ],
    "files": [
      "!**/*.{jsx,scss}"
    ],
    "publish": [
      {
        "provider": "s3",
        "bucket": "beamstudio",
        "path": "/${os}${env.PUBLISH_PATH}"
      }
    ],
    "mac": {
      "category": "com.flux3dp.beam-studio-flux",
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "build/entitlements.mac.plist",
      "entitlementsInherit": "build/entitlements.mac.plist",
      "target": [
        "dmg",
        "zip"
      ],
      "icon": "public/logo.icns"
    },
    "linux": {
      "category": "Graphics",
      "description": "Beam Studio",
      "vendor": "FLUX Inc.",
      "target": [
        "deb",
        "AppImage"
      ],
      "icon": "app-icons"
    },
    "win": {
      "icon": "app-icons/win-icon.ico",
      "certificateSubjectName": "FLUX TECHNOLOGY INC.",
      "publisherName": "FLUX TECHNOLOGY INC."
    },
    "dmg": {
      "artifactName": "Beam Studio ${version}.${ext}",
      "sign": false
    },
    "deb": {
      "depends": [
        "libfontconfig1-dev"
      ]
    },
    "nsis": {
      "include": "win-install.nsh",
      "artifactName": "Beam Studio Installer ${version}.${ext}"
    }
  }
}
