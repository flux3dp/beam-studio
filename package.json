{
  "name": "beam-studio",
  "productName": "Beam Studio",
  "version": "1.5.1-beta",
  "author": {
    "name": "FLUX Inc.",
    "email": "support@flux3dp.com"
  },
  "description": "Beam Studio",
  "private": true,
  "main": "electron-main.js",
  "dependencies": {
    "@simonwep/pickr": "^1.2.6",
    "@types/backbone": "^1.4.4",
    "@types/electron": "^1.6.10",
    "@types/html2canvas": "^0.0.35",
    "animate.css": "^4.1.0",
    "browserify": "^16.5.2",
    "bufferpack": "^0.0.6",
    "classnames": "^2.2.6",
    "cropperjs": "^1.5.9",
    "custom-electron-titlebar": "^3.2.2",
    "electron": "8.5.5",
    "electron-notarize": "^0.2.0",
    "electron-store": "^6.0.0",
    "electron-updater": "^4.2.0",
    "flux": "^3.1.3",
    "font-scanner": "dkdkwizard/font-scanner",
    "fontkit": "^1.8.1",
    "gulp-notify": "^3.2.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "jimp": "^0.10.3",
    "jquery": "^1.11.0",
    "jsencrypt": "^3.0.0-rc.1",
    "net-ping": "^1.2.3",
    "node-machine-id": "^1.1.12",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-contextmenu": "^2.13.0",
    "react-dom": "^16.13.0",
    "react-draggable": "^4.4.3",
    "react-select": "^3.1.0",
    "request": "^2.88.2",
    "rxjs": "^6.6.3",
    "serialport": "^8.0.5",
    "socket.io-client": "^1.4.6",
    "squirejs": "^0.2.1",
    "sudo-prompt": "^9.0.0",
    "text-to-svg": "^3.1.5",
    "typescript": "^4.0.2",
    "typings": "^2.1.1",
    "ws": "^7.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "ajv": "^5.5.2",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^8.1.0",
    "electron-builder": "22.3.6",
    "electron-publisher-s3": "^19.53.1",
    "electron-rebuild": "^1.8.5",
    "eslint": "^4.9.0",
    "eslint-plugin-react": "^7.4.0",
    "expect": "^1.14.0",
    "gulp": "^4.0.2",
    "gulp-babel": "8.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-exec": "^5.0.0",
    "gulp-mocha": "~2.2.0",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "~1.6.0",
    "gulp-uglify": "~1.5.4",
    "gulp-webserver": "~0.9.1",
    "mocha": "^2.3.4",
    "pump": "~1.0.1",
    "q": "~1.4.1",
    "requirejs": "~2.1.22",
    "webpack": "^5.11.0"
  },
  "scripts": {
    "test": "mocha '_test/unit/slicer.js' --compilers js:babel-core/register",
    "start": "electron .",
    "start-debug": "electron . --debug",
    "start-server": "electron . --server",
    "start-monitor": "electron . --monitor",
    "postinstall": "electron-rebuild",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.flux3dp.beam-studio-flux",
    "generateUpdatesFilesForAllChannels": true,
    "afterSign": "./scripts/notarize.js",
    "extraResources": [
      "backend",
      "utils"
    ],
    "files": [
      "!**/*.{jsx,scss}"
    ],
    "publish": [
      {
        "provider": "s3",
        "bucket": "beamstudio",
        "path": "/${os}${env.PUBLISH_PATH}"
      }
    ],
    "mac": {
      "category": "com.flux3dp.beam-studio-flux",
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "build/entitlements.mac.plist",
      "entitlementsInherit": "build/entitlements.mac.plist",
      "target": [
        "dmg",
        "zip"
      ],
      "icon": "public/logo.icns"
    },
    "linux": {
      "category": "Graphics",
      "description": "Beam Studio",
      "vendor": "FLUX Inc.",
      "target": [
        "deb",
        "AppImage"
      ],
      "icon": "app-icons"
    },
    "win": {
      "icon": "app-icons/win-icon.ico",
      "certificateSubjectName": "FLUX TECHNOLOGY INC.",
      "publisherName": "FLUX TECHNOLOGY INC."
    },
    "dmg": {
      "artifactName": "Beam Studio ${version}.${ext}",
      "sign": false
    },
    "deb": {
      "depends": [
        "libfontconfig1-dev"
      ]
    },
    "nsis": {
      "include": "win-install.nsh",
      "artifactName": "Beam Studio Installer ${version}.${ext}"
    }
  }
}
