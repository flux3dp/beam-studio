    "custom-electron-titlebar": "^4.1.5",
    "electron-store": "^8.1.0",
    "electron-updater": "^4.2.0",
    "eventemitter3": "^4.0.7",
    "font-scanner": "^0.2.1",
    "fontkit": "^1.8.1",
    "gl-matrix": "^3.3.0",
    "hammerjs": "^2.0.8",
    "jimp": "^0.10.3",
    "jquery": "^1.11.0",
    "jsencrypt": "3.0.0-rc.1",
    "mini-css-extract-plugin": "^1.6.0",
    "net-ping": "dkdkwizard/node-net-ping",
    "node-machine-id": "^1.1.12",
    "paper": "^0.12.17",
    "prop-types": "^15.7.2",
    "rc-table": "^7.28.3",
    "react": "^16.9.0",
    "react-dom": "^16.13.0",
    "react-draggable": "^4.4.3",
    "react-router-dom": "^5.2.0",
    "react-select": "^3.1.0",
    "request": "^2.88.2",
    "rxjs": "^6.6.3",
    "socket.io-client": "^1.4.6",
    "sprintf-js": "^1.1.2",
    "squirejs": "^0.2.1",
    "sudo-prompt": "^9.0.0",
    "typescript": "^4.9.5",
    "typings": "^2.1.1",
    "wdio-spec-reporter": "^0.1.5",
    "ws": "^7.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@electron/rebuild": "^3.2.10",
    "@types/classnames": "^2.3.1",
    "@types/electron": "^1.6.10",
    "@types/eventemitter3": "^2.0.2",
    "@types/hammerjs": "^2.0.40",
    "@types/jest": "^26.0.22",
    "@types/react": "^17.0.5",
    "@types/react-router-dom": "^5.1.7",
    "@types/sprintf-js": "^1.1.2",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "@wdio/cli": "^8.6.2",
    "@wdio/local-runner": "^8.6.0",
    "@wdio/mocha-framework": "^8.5.6",
    "@wdio/sauce-service": "^8.6.2",
    "@wdio/spec-reporter": "^8.4.0",
    "ajv": "^5.5.2",
    "babel-loader": "^8.1.0",
    "chromedriver": "^111.0.0",
    "electron": "^22.0.0",
    "electron-builder": "22.10.5",
    "electron-publisher-s3": "^19.53.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^7.24.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "expect": "^1.14.0",
    "file-loader": "^6.2.0",
    "jest": "^26.6.3",
    "mocha": "^2.3.4",
    "q": "~1.4.1",
    "sass": "^1.58.1",
    "sass-loader": "^13.2.0",
    "selenium-webdriver": "^4.8.1",
    "spectron": "^16.0.0",
    "style-loader": "^3.3.1",
    "ts-jest": "^26.5.5",
    "ts-loader": "^9.1.1",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "wdio-chromedriver-service": "^8.1.1",
    "wdio-electron-service": "^4.0.0",
    "webpack": "^5.11.0",
    "webpack-cli": "^4.6.0",
    "worker-loader": "^3.0.8"
  },
  "scripts": {
    "dev": "./node_modules/.bin/webpack watch",
    "test-spectron": "jest ./test/test.js",
    "start": "electron .",
    "start-debug": "electron . --debug",
    "start-server": "electron . --server",
    "start-monitor": "electron . --monitor",
    "postinstall": "electron-rebuild",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "test": "jest --passWithNoTests",
    "test-debug": "DEBUG_MODE=1 jest --passWithNoTests",
    "spec": "jest --config jest.config.spec.only.js --passWithNoTests",
    "integration": "jest --config jest.config.integration.only.js --passWithNoTests",
    "eslint": "eslint",
    "wdio": "wdio run ./test/wdio.conf.ts"
  },
  "build": {
    "appId": "com.flux3dp.beam-studio-flux",
    "protocols": {
      "name": "beam-studio-protocol",
      "schemes": [
        "beam-studio"
      ]
    },
    "generateUpdatesFilesForAllChannels": true,
    "afterSign": "./scripts/notarize.js",
    "extraResources": [
      "backend",
      "utils"
    ],
    "files": [
      "!**/*.{jsx,scss}"
    ],
    "publish": [
      {
        "provider": "s3",
        "bucket": "beamstudio",
        "path": "/${os}${env.PUBLISH_PATH}"
      }
    ],
    "mac": {
      "category": "com.flux3dp.beam-studio-flux",
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "build/entitlements.mac.plist",
      "entitlementsInherit": "build/entitlements.mac.plist",
      "target": [
        "dmg",
        "zip"
      ],
      "icon": "public/logo.icns"
    },
    "linux": {
      "category": "Graphics",
      "description": "Beam Studio",
      "vendor": "FLUX Inc.",
      "target": [
        "deb",
        "AppImage"
      ],
      "icon": "app-icons"
    },
    "win": {
      "icon": "app-icons/win-icon.ico",
      "certificateSubjectName": "FLUX TECHNOLOGY INC.",
      "publisherName": "FLUX TECHNOLOGY INC."
    },
    "dmg": {
      "artifactName": "Beam Studio ${version}.${ext}",
      "sign": false
    },
    "deb": {
      "depends": [
        "libfontconfig1-dev"
      ]
    },
    "nsis": {
      "include": "win-install.nsh",
      "artifactName": "Beam Studio Installer ${version}.${ext}"
    }
  }
}
